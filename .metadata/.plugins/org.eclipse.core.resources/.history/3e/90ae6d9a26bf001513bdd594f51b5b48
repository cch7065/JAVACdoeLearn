package VolcanoRobot;

public class Test2 extends TestFinalMethod { 
    
	public void f1(){     
	    System.out.println("Test1父类方法f1被覆盖!"); 
	} 
	public void f3(){
		System.out.println("test2 can override the father's Method");
	}
	public void f2(){
		System.out.println("test2 can't override the father's Method");
	}

	
public static void main(String[] args) { 
    // TODO 自动生成方法存根 
	TestFinalMethod t1 = new TestFinalMethod();
	t1.f1();
	t1.f2();
	t1.f3();
	Test2 t2 = new Test2();
	t2.f1();
	t2.f2();
	t2.f3();
	
} 
}
